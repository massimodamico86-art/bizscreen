{
  "name": "bizscreen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test && node scripts/seed-ci-test-user.cjs && npm run test:e2e",
    "test:ci:local": "npm run test && npm run test:e2e",
    "generate:sitemap": "node scripts/generate-sitemap.cjs",
    "load-test": "node load-tests/run-all.js",
    "load-test:quick": "node load-tests/run-all.js --quick",
    "load-test:auth": "node load-tests/auth-burst.js",
    "load-test:heartbeat": "k6 run load-tests/heartbeat.js",
    "load-test:playlist": "k6 run load-tests/playlist-resolution.js",
    "load-test:media": "node load-tests/media-library.js",
    "build:polotno": "cd scripts/polotno-build && npm run build",
    "svg:convert": "node scripts/svg-to-polotno.cjs",
    "analyze": "npm run build && open perf-reports/bundle-stats.html"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.946.0",
    "@aws-sdk/s3-request-presigner": "^3.946.0",
    "@date-fns/tz": "^1.4.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2",
    "@sentry/react": "^10.36.0",
    "@smastrom/react-rating": "^1.5.0",
    "@supabase/supabase-js": "^2.80.0",
    "date-fns": "^4.1.0",
    "fabric": "^6.9.0",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "ical.js": "^2.2.1",
    "idb": "^8.0.3",
    "isomorphic-dompurify": "^2.35.0",
    "lucide-react": "^0.548.0",
    "polotno": "^2.33.2",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5",
    "resend": "^6.8.0",
    "stripe": "^20.0.0",
    "uuid": "^13.0.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.3.0",
    "msw": "^2.12.3",
    "patch-package": "^8.0.1",
    "pg": "^8.17.2",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "terser": "^5.46.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.14"
  }
}
